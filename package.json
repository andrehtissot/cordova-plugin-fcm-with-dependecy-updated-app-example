{
  "name": "com.andretissot.cordovafcm.exampleapp",
  "displayName": "Cordova FCM Push Plugin App Example",
  "cordova_name": "Cordova FCM Push Plugin App Example",
  "version": "0.0.1",
  "description": "A sample Apache Cordova application that makes use of cordova-plugin-fcm-with-dependecy-updated",
  "main": "index.js",
  "scripts": {
    "start": "cordova run android"
  },
  "keywords": [
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios",
    "notifications",
    "push",
    "firebase",
    "fcm",
    "ios",
    "android",
    "cordova"
  ],
  "license": "MIT",
  "author": "Andr√© Augusto Tissot",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrehtissot/cordova-plugin-fcm-with-dependecy-updated-app-example"
  },
  "dependencies": {
    "cordova-android": "^8.0.0",
    "cordova-plugin-fcm-with-dependecy-updated": "^3.0.0"
  },
  "devDependencies": {
    "cordova-plugin-whitelist": "^1.3.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-fcm-with-dependecy-updated": {
        "FCM_CORE_VERSION": "16.0.9",
        "FCM_VERSION": "18.0.0",
        "GRADLE_TOOLS_VERSION": "2.3.+",
        "GOOGLE_SERVICES_VERSION": "3.0.0"
      }
    },
    "platforms": [
      "android"
    ]
  }
}