{
  "name": "com.andretissot.cordovafcm.exampleapp",
  "displayName": "Cordova FCM Push Plugin App Example",
  "cordova_name": "Cordova FCM Push Plugin App Example",
  "version": "0.0.1",
  "description": "A sample Apache Cordova application that makes use of cordova-plugin-fcm-with-dependecy-updated",
  "main": "index.js",
  "scripts": {
    "start": "cordova run android",
    "postinstall": "cp node_modules/promise-polyfill/dist/polyfill.min.js www"
  },
  "keywords": [
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios",
    "notifications",
    "push",
    "firebase",
    "fcm",
    "ios",
    "android",
    "cordova"
  ],
  "license": "MIT",
  "author": "Andr√© Augusto Tissot",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrehtissot/cordova-plugin-fcm-with-dependecy-updated-app-example"
  },
  "dependencies": {
    "cordova-plugin-whitelist": "1",
    "promise-polyfill": "^8.2.0"
  },
  "devDependencies": {
    "cordova-android": "^9.0.0",
    "cordova-ios": "^6.1.1",
    "cordova-plugin-fcm-with-dependecy-updated": "^7.4.0",
    "cordova-plugin-whitelist": "^1.3.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-fcm-with-dependecy-updated": {
        "ANDROID_DEFAULT_NOTIFICATION_ICON": "@mipmap/ic_launcher",
        "ANDROID_FCM_VERSION": "21.0.0",
        "ANDROID_FIREBASE_BOM_VERSION": "26.0.0",
        "ANDROID_GOOGLE_SERVICES_VERSION": "4.3.4",
        "ANDROID_GRADLE_TOOLS_VERSION": "4.1.0"
      }
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}