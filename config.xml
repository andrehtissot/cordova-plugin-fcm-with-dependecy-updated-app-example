<?xml version='1.0' encoding='utf-8'?>
<widget id="com.andretissot.cordovafcm.exampleapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cordova FCM Push Plugin App Example</name>
    <description>
        A sample Apache Cordova application that makes use of cordova-plugin-fcm-with-dependecy-updated
    </description>
    <author email="andre_a_tissot@yahoo.com.br">Andr√© Augusto Tissot</author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
